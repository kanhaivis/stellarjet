<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ns.stellarjet">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_stellar_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_stellar_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".SplaceScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name="com.ns.activity.LoginActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.ns.activity.TouchIdActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.ns.activity.HomeActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.PinActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.BookFlightActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.PurchaseActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.DrawerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.BoardingPassActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.MyBookingsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.MyBookingUserDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.PreferencesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.ManagerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.DiningPreferencesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.BookingConfirmedActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ns.activity.SignUpActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.ns.activity.PrimaryUserListActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.ns.activity.TicketViewActivity"
            android:screenOrientation="portrait" />

        <!-- razor pay -->
        <meta-data
            android:name="com.razorpay.ApiKey"
            android:value="rzp_test_wgZ2yUsDJG9uYz" />

    </application>

</manifest>